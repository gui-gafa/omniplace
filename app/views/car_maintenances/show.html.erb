<p id="notice"><%= notice %></p>

<p>
  <strong>Value:</strong>
  <%= @car_maintenance.value %>
</p>

<p>
  <strong>Description:</strong>
  <%= @car_maintenance.description %>
</p>

<p>
  <strong>Date:</strong>
  <%= @car_maintenance.date %>
</p>

<p>
  <strong>Warranty:</strong>
  <%= @car_maintenance.warranty %>
</p>

<p>
  <strong>Car:</strong>
  <%= @car_maintenance.car.name %>
</p>

<% @car_maintenance.documents.each do |document| %>
  <%= link_to document.filename, rails_blob_path(document, disposition: "preview"), class: "btn btn-info", target: "_blank"%>
<% end if @car_maintenance.documents.attached?%>
<%= link_to 'Edit', edit_car_maintenance_path(@car_maintenance) %> |
